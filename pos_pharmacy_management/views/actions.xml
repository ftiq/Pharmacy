<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_medicine_products" model="ir.actions.act_window">
        <field name="name">Medicine Products</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">product.product</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('is_medicine','=',True)]</field>
        <field name="context">{'default_is_pharma_product': True, 'default_is_medicine' : True}</field>
    </record>

