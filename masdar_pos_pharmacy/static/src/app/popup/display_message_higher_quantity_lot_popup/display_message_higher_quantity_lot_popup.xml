<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="masdar_pos_pharmacy.DisplayMessageHigherQuantityLotPopup" owl="1">
        <div class="popup">
            <header class="modal-header">
                <t class="modal-title" t-esc="props.title" />
            </header>
            <main class="modal-body">
                    <div>
                        <span>Selected quantity exceeds available quantity in lot <span class="text-info" t-esc="props.quantitylot"/></span>
                    </div>
            </main>
            <footer class="footer modal-footer">
                <div class="button cancel btn btn-lg" t-on-click="cancel"> Cancel </div>
            </footer>
        </div>
    </t>
</templates>